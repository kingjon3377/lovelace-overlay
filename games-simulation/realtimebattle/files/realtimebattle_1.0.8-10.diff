@@ -0,0 +1,93 @@
+#!/usr/bin/make -f
+
+d=debian/tmp
+ROOT=$(d)/usr
+LC_ALL=C
+PACKAGE=realtimebattle
+export JAVAC=/usr/bin/ecj
+export ROBOTS=empty rotate_and_fire seek_and_destroy xt-bot thomas2 joypad_robot perl-Skeleton perl raziel $(shell [ -x $(JAVAC) ] && echo jBot )
+
+build: build-stamp
+build-stamp:
+	dh_testdir
+
+	./configure --prefix=/usr --with-rtb-dir=/usr/lib/realtimebattle LDFLAGS=-Wl,--as-needed
+
+	touch config.h.in
+	if [ ! -x $(JAVAC) ]; then export ROBOTS="empty rotate_and_fire seek_and_destroy xt-bot thomas2 joypad_robot perl-Skeleton perl raziel"; fi
+	$(MAKE)
+
+	touch build-stamp
+
+clean: clean1
+clean1:
+	dh_testdir
+	dh_testroot
+	rm -f build-stamp install-stamp
+
+	[ ! -f Makefile ] || $(MAKE) distclean
+#	this file is created and not cleaned by the rtb Makefile
+	-rm Documentation/German/RealTimeBattle.info
+	dh_clean
+
+install: install-stamp
+install-stamp: build-stamp
+	dh_testdir
+	dh_testroot
+	dh_clean -k
+	dh_installdirs
+
+	$(MAKE) install \
+		DESTDIR=`pwd`/$(d)/
+#	this file is created empty
+	-rm $(d)/usr/lib/realtimebattle/Documentation/German/RealTimeBattle.info
+
+	dh_installdocs
+	dh_installinfo -n
+	dh_installman debian/realtimebattle.6
+	dh_installmenu
+	dh_installchangelogs ChangeLog
+
+	touch install-stamp
+
+# Build architecture-independent files here.
+binary-indep: build install
+	dh_testdir
+	dh_testroot
+	dh_install -i
+	mv debian/realtimebattle-common/usr/share/pixmaps/RealTimeBattle.xpm \
+	  debian/realtimebattle-common/usr/share/pixmaps/realtimebattle.xpm
+
+	dh_link -i
+	dh_strip -i
+	dh_compress -i
+	dh_fixperms -i
+	dh_installdeb -i
+	dh_shlibdeps -i
+	dh_gencontrol -i
+	dh_md5sums -i
+	dh_builddeb -i
+
+# Build architecture-dependent files here.
+binary-arch: build install
+	dh_testdir
+	dh_testroot
+	dh_install -a
+	install -m 644 debian/realtimebattle.override $(CURDIR)/debian/realtimebattle/usr/share/lintian/overrides/realtimebattle
+
+	dh_link -a
+	dh_strip -a
+	dh_compress -a
+	dh_fixperms -a
+	dh_installdeb -a
+	dh_shlibdeps -a
+	dh_gencontrol -a
+	dh_md5sums -a
+	dh_builddeb -a
+
+source diff:
+	@echo >&2 'source and diff are obsolete - use dpkg-source -b'; false
+
+binary: binary-indep binary-arch
+.PHONY: build clean binary-indep binary-arch binary install clean1
+
